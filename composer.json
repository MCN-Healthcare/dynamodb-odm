{
    "name"        : "oasis/dynamodb-odm",
    "description" : "ODM for dynamodb",
    "type"        : "library",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dboerger/DynamoDB-aws-wrappers"
        }
    ],
    "require"     : {
        "php"                 : "^7.1.3",
        "doctrine/annotations": "^1.4",
        "oasis/aws-wrappers"  : "dev-Timestampable",
        "oasis/logging"       : "^1.0",
        "doctrine/common"     : "^2.7",
        "symfony/console"     : "^4.2",
        "symfony/finder"      : "^4.2"
    },
    "require-dev" : {
        "phpunit/phpunit": "^5.7",
        "liip/functional-test-bundle": "^1.10",
        "symfony/phpunit-bridge": ">=4.1"
    },
    "license"     : "MIT",
    "authors"     : [
        {
            "name" : "Minhao Zhang",
            "email": "minhao.zhang@qq.com"
        }
    ],
    "autoload"    : {
        "psr-4": {
            "Oasis\\Mlib\\ODM\\Dynamodb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Oasis\\Mlib\\ODM\\Dynamodb\\Ut\\": "ut/"
        }
    },
    "bin"         : [
        "bin/oasis-dynamodb-odm"
    ]
}
